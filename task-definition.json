{
    "family": "${TASK_DEFINITION_NAME}",
    "containerDefinitions": [
        {
            "image": "348722393091.dkr.ecr.us-west-2.amazonaws.com/nodejs_project:${env.BUILD_ID}",
            "name": "nodejs_container",
	        "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::348722393091:role/ECS_ECR"
}